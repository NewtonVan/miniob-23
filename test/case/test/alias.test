select t1.id as num, t1.col1 as age, t1.feat1, t2.* from table_alias_1 t1, table_alias_2 t2 where t1.id < t2.id;

select * from table_alias_1 t1 where id in (select t2.id from table_alias_2 t2 where t2.col2 >= t1.col1);

select * as alias from table_alias_1 t1;

CREATE TABLE table_alias_one_row(id int);

select * as alias from table_alias_one_row t1;

select t1.* as alias, id as alias from table_alias_one_row t1;

select t1.id as num from table_alias_1 t1 where id in (select t2.id from table_alias_2 t2 where num > t2.id);
